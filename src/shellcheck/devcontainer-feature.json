{
    "name": "ShellCheck",
    "id": "shellcheck",
    "version": "1.0.0",
    "description": "Install ShellCheck from GitHub releases.",
    "options": {
        "tag": {
            "default": "latest",
            "type": "string",
            "description": "Tag of the GitHub release to install."
        },
        "hash": {
            "default": "none",
            "type": "string",
            "description": "Comma-separated list of SHA256 hashes of the release asset."
        }
    },
    "dependsOn": {
        "ghcr.io/devcontainers/features/node:1.6.1": {},
        "ghcr.io/doughom/features/common-utils:1": {}
    }
}
